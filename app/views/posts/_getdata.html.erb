<div id="get_response">
  <label>
    <%= t(:expected_result) %><br />
  </label>
  <br />
  <pre lang="zh">
    <%= @wrongmsg.force_encoding("UTF-8") unless @wrongmsg.nil? %>
  </pre>
  <pre lang="zh">
    <p style = "color:red">
    <% if @error %>
      <%= @error%>
    <% end %>
    </p>
  <%if !@preview_result.nil? %>
    <% if @preview_result.class == String %>
      <%= JSON.parse(@preview_result.force_encoding("UTF-8")) %>
    <% else %>
      <%= JSON.pretty_generate(JSON.parse(@preview_result.force_encoding("UTF-8"))) unless @preview_result.nil? %>
    <% end %>
  <% end %>
  </pre>
</div>

